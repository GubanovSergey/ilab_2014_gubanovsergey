#include <stdio.h>
#include <math.h>
int main()
{
	double a, b, c, x1, x2, d, n;
	scanf("%lg %lg %lg", &a, &b, &c);
	if ((a == 0) && (b == 0))
	{
		if (c == 0) { printf("Uncountable roots amount"); n = 3; }
		else { printf("No roots"); n = 0; }
	}
	else if (a == 0)
	{
		x1 = -c / b;
		n = 1;
	}
	else
	{
		d = b*b - 4 * a*c;
		if (d < 0)
        {
            printf("No roots");
            n = 0;
        }
		else if (d == 0)
        {
            x1 = -b / (2 * a);
            printf("There is 1 root: %lg", x1);
            n = 1;
        }
		else
		{
			x1 = (-b - sqrt(d)) / (2 * a);
			x2 = (-b + sqrt(d)) / (2 * a);
			printf("There are 2 roots: %lg and %lg", x1, x2);
			n = 2;
		}
	}
	return 0;
}
